{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "e84807d5-bede-410b-80d9-047ba85533c1"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "zerofiltre-blog": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/zerofiltre-blog/browser",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets",
              "src/assets/favicon/favicon.ico", 
              "src/assets/sitemap.xml",
              "src/assets/robots.txt"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css",
              "node_modules/prismjs/themes/prism-okaidia.css"
            ],
            "scripts": [
              "node_modules/prismjs/prism.js",
              "node_modules/prismjs/plugins/toolbar/prism-toolbar.js",
              "node_modules/prismjs/plugins/copy-to-clipboard/prism-copy-to-clipboard.js",
              "node_modules/prismjs/components/prism-typescript.min.js",
              "node_modules/prismjs/components/prism-csharp.min.js",
              "node_modules/prismjs/components/prism-java.min.js",
              "node_modules/prismjs/components/prism-css.min.js",
              "node_modules/prismjs/components/prism-python.min.js",
              "node_modules/prismjs/components/prism-javascript.min.js",
              "node_modules/prismjs/components/prism-yaml.min.js",
              "node_modules/prismjs/components/prism-docker.min.js"
            ]
          },
          "configurations": {
            "prod": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "uat": {
              "optimization": {
                "scripts": true,
                "styles": { "minify": true, "inlineCritical": false },
                "fonts": true
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "dev": {
              "optimization": false,
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": false,
              "vendorChunk": true,
              "buildOptimizer": false
            },
            "local": {
              "optimization": false,
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": false,
              "vendorChunk": true,
              "buildOptimizer": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.locals.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "prod"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "prod": {
              "browserTarget": "zerofiltre-blog:build:prod"
            },
            "uat": {
              "browserTarget": "zerofiltre-blog:build:uat"
            },
            "dev": {
              "browserTarget": "zerofiltre-blog:build:dev"
            },
            "local": {
              "browserTarget": "zerofiltre-blog:build:local"
            }
          },
          "defaultConfiguration": "local"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "zerofiltre-blog:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": ["src/favicon.ico", "src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/zerofiltre-blog/server",
            "main": "server.ts",
            "tsConfig": "tsconfig.server.json"
          },
          "configurations": {
            "prod": {
              "outputHashing": "media",
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true
            },
            "uat": {
              "outputHashing": "media",
              "optimization": true,
              "sourceMap": false,
              "extractLicenses": true
            },
            "dev": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false
            },
            "local": {
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.locals.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "prod"
        },
        "serve-ssr": {
          "builder": "@nguniversal/builders:ssr-dev-server",
          "configurations": {
            "local": {
              "browserTarget": "zerofiltre-blog:build:local",
              "serverTarget": "zerofiltre-blog:server:local"
            },
            "dev": {
              "browserTarget": "zerofiltre-blog:build:dev",
              "serverTarget": "zerofiltre-blog:server:dev"
            },
            "uat": {
              "browserTarget": "zerofiltre-blog:build:uat",
              "serverTarget": "zerofiltre-blog:server:uat"
            },
            "prod": {
              "browserTarget": "zerofiltre-blog:build:prod",
              "serverTarget": "zerofiltre-blog:server:prod"
            }
          },
          "defaultConfiguration": "local"
        },
        "prerender": {
          "builder": "@nguniversal/builders:prerender",
          "options": {
            "routes": ["/"]
          },
          "configurations": {
            "production": {
              "browserTarget": "zerofiltre-blog:build:production",
              "serverTarget": "zerofiltre-blog:server:production"
            },
            "development": {
              "browserTarget": "zerofiltre-blog:build:development",
              "serverTarget": "zerofiltre-blog:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  }
}
